<section class="mt-5">
    <h1 class="text-danger"> Lista:</h1>
        <% if (productos.length === 0) { %> 
            <h2 class="bg-danger rounded text-center m-4">No se encontraron datos</h2>
        <% } else {%> 
    <table class="table table-dark">
        <tr class="text-warning">
            <th>Item</th>
            <th>Price</th>
            <th>Thumbnail</th>
        </tr>
        <% productos.forEach(function(user) { %> 
        <tr>
            <td><%= user.title %></td>
            <td><%= user.price %></td>
            <td><img src=<%= user.thumbnail %> alt="Image" width="50px"></td>
        </tr>
        <% }) %> 
    </table>
    <% } %> 
</section>