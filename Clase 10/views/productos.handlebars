<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        {{#each .}}
            <tr>
                <td>{{id}}</td>
                <td>{{title}}</td>
                <td>{{price}}</td>
                <td><img src={{thumbnail}} alt="Image" width="50px"></td>
            </tr>
        {{/each}}
    </tbody>
</table>